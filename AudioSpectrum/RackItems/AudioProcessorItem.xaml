<rackItems:RackItemBase
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             xmlns:rackItems="clr-namespace:AudioSpectrum.RackItems"
             x:Class="AudioSpectrum.RackItems.AudioProcessorItem"
             mc:Ignorable="d" 
             Height="40">
    <d:RackItemBase.DesignWidth>350</d:RackItemBase.DesignWidth>
    <Grid Background="#feee">
        <Grid Margin="10">
            <xctk:IntegerUpDown x:Name="NumberOfSamplesUpDown" Value="1" Margin="255,0,10,0" VerticalAlignment="Top" Minimum="1" Maximum="100" Increment="1" Height="20"/>
            <Label Content="Number of previous samples to average from" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,-4,0,0"/>
        </Grid>
    </Grid>
</rackItems:RackItemBase>

